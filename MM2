local function SPNLPQB_fake_script() 
    local script = Instance.new('LocalScript', Activation_3)
    local infiniteJump = false
    local mouse = game.Players.LocalPlayer:GetMouse()

    local function setInfiniteJump(state)
        infiniteJump = state
        toggleButton(script.Parent, infiniteJump)
    end

    script.Parent.MouseButton1Click:connect(function()
        setInfiniteJump(not infiniteJump)
    end)

    mouse.KeyDown:Connect(function(k)
        if k:lower() == "v" then
            setInfiniteJump(not infiniteJump)
        end
    end)

    game:GetService("UserInputService").JumpRequest:connect(function()
        if infiniteJump and game.Players.LocalPlayer.Character then
            game.Players.LocalPlayer.Character:FindFirstChildOfClass('Humanoid'):ChangeState("Jumping")
        end
    end)
end
coroutine.wrap(SPNLPQB_fake_script)()


local function COIN_FARM_SCRIPT()
    local script = Instance.new('LocalScript', Activation_4)
    local LocalPlayer = game:GetService("Players").LocalPlayer
    local RunService = game:GetService("RunService")

    local isFlying = false
    local flyVelocity = nil
    local flyGyro = nil
    local currentTarget = nil
    local farmConnection = nil
    local noclipConnection = nil 
    local isFarming = false
    local FLY_SPEED = 350 

    local function findCoinContainer()
        for _, v in pairs(workspace:GetDescendants()) do
            if v.Name == "CoinContainer" then
                return v
            end
        end
        return nil
    end

    local function getNextCoin()
        local container = findCoinContainer()
        if not container then return nil end
        for _, coin in pairs(container:GetChildren()) do
            if coin:IsA("BasePart") then
                return coin
            end
        end
        return nil
    end

    
    local function startNoclipping()
        if noclipConnection then return end
        noclipConnection = RunService.Stepped:Connect(function()
            local char = LocalPlayer.Character
            if not char then return end
            for _, part in pairs(char:GetDescendants()) do
                if part:IsA("BasePart") then
                    part.CanCollide = false
                end
            end
        end)
    end

    
    local function stopNoclipping()
        if noclipConnection then
            noclipConnection:Disconnect()
            noclipConnection = nil
        end
    end

    local function startFlying()
        local char = LocalPlayer.Character
        if not char or isFlying then return end
        local hrp = char:FindFirstChild("HumanoidRootPart")
        local humanoid = char:FindFirstChild("Humanoid")
        if not (hrp and humanoid) then return end

        isFlying = true
        humanoid.PlatformStand = true

        flyGyro = Instance.new("BodyGyro", hrp)
        flyGyro.MaxTorque = Vector3.new(math.huge, math.huge, math.huge)
        flyGyro.P = 50000

        flyVelocity = Instance.new("BodyVelocity", hrp)
        flyVelocity.MaxForce = Vector3.new(math.huge, math.huge, math.huge)
        flyVelocity.Velocity = Vector3.zero
    end

    local function stopFlying()
        isFlying = false
        currentTarget = nil
        if flyVelocity then flyVelocity:Destroy() end
        if flyGyro then flyGyro:Destroy() end
        local char = LocalPlayer.Character
        if char and char:FindFirstChild("Humanoid") then
            char.Humanoid.PlatformStand = false
        end
    end

    local function farmLoop()
        if not isFlying or not flyVelocity or not flyGyro then return end
        local char = LocalPlayer.Character
        local hrp = char and char:FindFirstChild("HumanoidRootPart")
        if not hrp then stopFlying() return end

        if not currentTarget or not currentTarget.Parent then
            currentTarget = getNextCoin()
        end

        if currentTarget then
            flyGyro.CFrame = CFrame.new(hrp.Position, currentTarget.Position)
            local dir = (currentTarget.Position - hrp.Position).Unit
            flyVelocity.Velocity = dir * FLY_SPEED
        else
            flyVelocity.Velocity = Vector3.zero
        end
    end

    script.Parent.MouseButton1Click:Connect(function()
        isFarming = not isFarming
        toggleButton(script.Parent, isFarming)

        if isFarming then
            startFlying()
            startNoclipping() 
            farmConnection = RunService.RenderStepped:Connect(farmLoop)
        else
            if farmConnection then
                farmConnection:Disconnect()
                farmConnection = nil
            end
            stopFlying()
            stopNoclipping() 
        end
    end)
end
coroutine.wrap(COIN_FARM_SCRIPT)()



local function CKAW_fake_script() 
    local script = Instance.new('LocalScript', Activation_5)
    local toggle = false
    local obj = game.workspace

    function XrayOn(obj)
        for _, v in pairs(obj:GetChildren()) do
            if (v:IsA("BasePart")) and not v.Parent:FindFirstChild("Humanoid") then
                v.LocalTransparencyModifier = 0.75
            end
            XrayOn(v)
        end
    end

    function XrayOff(obj)
        for _, v in pairs(obj:GetChildren()) do
            if (v:IsA("BasePart")) and not v.Parent:FindFirstChild("Humanoid") then
                                v.LocalTransparencyModifier = 0
            end
            XrayOff(v)
        end
    end

    script.Parent.MouseButton1Click:Connect(function()
        toggle = not toggle
        toggleButton(script.Parent, toggle)
        if toggle then
            XrayOn(obj)
        else
            XrayOff(obj)
        end
    end)
end
coroutine.wrap(CKAW_fake_script)()

local function ANTI_AFK_SCRIPT() 
    local script = Instance.new('LocalScript', Activation_6)
    script.Parent.MouseButton1Click:Connect(function()
        toggleButton(script.Parent, true)
        loadstring(game:HttpGet("https://raw.githubusercontent.com/FaresXForest/antiafk-by-flex-and-fares/refs/heads/main/faresxflex"))()
        wait(1)
        toggleButton(script.Parent, false)
    end)
