local function JIPBTDH_fake_script() -- BottomFrame.LocalScript 
	local script = Instance.new('LocalScript', BottomFrame)
    local tabs = {Tab1, Tab2, Tab3, Tab4}
    local frames = {MainFrame_2, PlayerFrame, VisualFrame, TeleportFrame}

    local function switchTab(selectedTab, selectedFrame)
        for i, tab in ipairs(tabs) do
            tab.BackgroundColor3 = (tab == selectedTab) and lightGray or darkPurple
        end
        for i, frame in ipairs(frames) do
            frame.Visible = (frame == selectedFrame)
        end
    end

	Tab1.MouseButton1Click:Connect(function() switchTab(Tab1, MainFrame_2) end)
	Tab2.MouseButton1Click:Connect(function() switchTab(Tab2, PlayerFrame) end)
	Tab3.MouseButton1Click:Connect(function() switchTab(Tab3, VisualFrame) end)
	Tab4.MouseButton1Click:Connect(function() switchTab(Tab4, TeleportFrame) end)
end
coroutine.wrap(JIPBTDH_fake_script)()


