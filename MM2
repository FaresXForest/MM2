local function LZHKGTA_fake_script() -- AimPlayer.Script 
	local script = Instance.new('LocalScript', AimPlayer)
	_G.aimingOn = false
	script.Parent.AimActivation.MouseButton1Click:Connect(function()
		_G.aimingOn = true
		local user = script.Parent.AimTB.Text
		while _G.aimingOn do
			local targetPlayer = game:GetService("Players"):FindFirstChild(user)
			if targetPlayer and targetPlayer.Character and targetPlayer.Character:FindFirstChild("HumanoidRootPart") and targetPlayer.Character.Humanoid.Health > 0 then
				workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, targetPlayer.Character.HumanoidRootPart.Position)
			else
				_G.aimingOn = false -- Stop if target is gone
			end
            wait()
		end
	end)
	
	script.Parent.DeAimActivation.MouseButton1Click:Connect(function()
		_G.aimingOn = false
	end)
end
coroutine.wrap(LZHKGTA_fake_script)()

local function MVWHPW_fake_script() 
    local script = Instance.new('LocalScript', VisualFrame)
    local ESPToggle = false
    local espConnection = nil

    local function ClearESP()
        for _, v in pairs(workspace:GetDescendants()) do
            if v.Name == "EGUI" then
                v:Destroy()
            end
        end
    end

    local function MakeESP()
        pcall(ClearESP)
        local plrs = game:GetService("Players")
        local localPlayer = plrs.LocalPlayer

        for _, v in pairs(plrs:GetPlayers()) do
            if v ~= localPlayer and v.Character and v.Character:FindFirstChild("Head") then
                local roleColor = Color3.new(1, 1, 1) 
                if v.Character:FindFirstChild("Knife") or v.Backpack:FindFirstChild("Knife") then
                    roleColor = Color3.new(1, 0, 0) 
                elseif v.Character:FindFirstChild("Gun") or v.Backpack:FindFirstChild("Gun") then
                    roleColor = Color3.new(0, 0, 1) 
                end

                local bgui = Instance.new("BillboardGui", v.Character.Head)
                bgui.Name = "EGUI"
                bgui.AlwaysOnTop = true
                bgui.ExtentsOffset = Vector3.new(0, 2, 0)
                bgui.Size = UDim2.new(0, 200, 0, 50)
                
                local nam = Instance.new("TextLabel", bgui)
                nam.Text = v.Name
                nam.BackgroundTransparency = 1
                nam.TextSize = 15
                nam.Font = Enum.Font.GothamBold
                nam.TextColor3 = roleColor
                nam.Size = UDim2.new(1, 0, 1, 0)
            end
        end
    end

    script.Parent.AllESP.Activation.MouseButton1Click:Connect(function()
        ESPToggle = not ESPToggle
        toggleButton(script.Parent.AllESP.Activation, ESPToggle)
        if ESPToggle then
            espConnection = game:GetService("RunService").RenderStepped:Connect(MakeESP)
        else
            if espConnection then
                espConnection:Disconnect()
                espConnection = nil
            end
            ClearESP()
        end
    end)
end
coroutine.wrap(MVWHPW_fake_script)()

