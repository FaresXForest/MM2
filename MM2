local function QHWFTYB_fake_script() -- Kill Murderer Script
	local script = Instance.new('LocalScript', Activation_7)
	local function GetMurderer()
	    for _, v in pairs(game:GetService("Players"):GetPlayers()) do
	        if v.Character and (v.Character:FindFirstChild("Knife") or v.Backpack:FindFirstChild("Knife")) then
	            return v
	        end
	    end
	end
	script.Parent.MouseButton1Click:Connect(function()
        toggleButton(script.Parent, true)
		local Murderer = GetMurderer() 
		if Murderer and Murderer.Character and Murderer.Character:FindFirstChild("HumanoidRootPart") then
			repeat
				if Murderer and Murderer.Character and Murderer.Character:FindFirstChild("HumanoidRootPart") then
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Murderer.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 5)
					workspace.CurrentCamera.CFrame = CFrame.new(workspace.CurrentCamera.CFrame.Position, Murderer.Character.HumanoidRootPart.Position)
				else
					break
				end
				wait()
			until not Murderer or not Murderer.Parent or not Murderer.Character or Murderer.Character.Humanoid.Health == 0
		end
        toggleButton(script.Parent, false)
	end)
end
coroutine.wrap(QHWFTYB_fake_script)()

local function AUTO_GRAB_GUN_SCRIPT()
    local script = Instance.new("LocalScript", Activation_8)
    local Players = game:GetService("Players")
    local RunService = game:GetService("RunService")
    local player = Players.LocalPlayer

    local running = false

    local function findGun()
        return workspace:FindFirstChild("GunDrop", true)
    end

    script.Parent.MouseButton1Click:Connect(function()
        running = not running

        toggleButton(script.Parent, running)

        task.spawn(function()
            while running do
                local char = player.Character
                if char and char:FindFirstChild("HumanoidRootPart") then
                    local hrp = char.HumanoidRootPart
                    local oldPos = hrp.CFrame

                    local gun = findGun()
                    if gun and gun:IsA("BasePart") then
                        hrp.CFrame = gun.CFrame + Vector3.new(0, 2, 0)
                        task.wait(0.3)
                        hrp.CFrame = oldPos
                        running = false
                        toggleButton(script.Parent, false)
                        break
                    end
                end
                task.wait(0.5) 
            end
        end)
    end)
end
coroutine.wrap(AUTO_GRAB_GUN_SCRIPT)()

local function GTOJAZG_fake_script() 
	local script = Instance.new('LocalScript', Activation_9)
	script.Parent.MouseButton1Click:Connect(function()
        toggleButton(script.Parent, true)
		local Players = game:GetService("Players")	
		for i, Victim in pairs(Players:GetPlayers()) do
			if Victim.Name ~= game.Players.LocalPlayer.Name and Victim.Character and Victim.Character:FindFirstChild("HumanoidRootPart") then
				repeat wait()
					game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = Victim.Character.HumanoidRootPart.CFrame * CFrame.new(0, 0, 1)
				until not Victim or not Victim.Parent or not Victim.Character or Victim.Character.Humanoid.Health == 0
			end
		end
        toggleButton(script.Parent, false)
	end)
end
